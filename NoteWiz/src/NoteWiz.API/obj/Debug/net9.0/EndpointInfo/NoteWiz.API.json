  "components": {
    "schemas": {
      "CreateNoteDTO": {
        "required": [
          "content",
          "title"
        ],
        "type": "object",
        "properties": {
          "title": {
            "maxLength": 200,
            "minLength": 0,
            "type": "string",
            "description": "Note title"
          },
          "content": {
            "minLength": 1,
            "type": "string",
            "description": "Note content"
          },
          "tags": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "color": {
            "pattern": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
            "type": "string",
            "nullable": true
          },
          "isPinned": {
            "type": "boolean"
          }
        },
        "additionalProperties": false,
        "description": "Data Transfer Object for creating a note"
      },
      "CreateNoteShareDTO": {
        "required": [
          "noteId",
          "sharedWithUserId"
        ],
        "type": "object",
        "properties": {
          "noteId": {
            "type": "integer",
            "description": "ID of the note to share",
            "format": "int32"
          },
          "sharedWithUserId": {
            "type": "integer",
            "description": "ID of the user to share the note with",
            "format": "int32"
          },
          "canEdit": {
            "type": "boolean",
            "description": "Whether the user can edit the note"
          }
        },
        "additionalProperties": false,
        "description": "Data Transfer Object for creating a note share"
      },
      "CreateTaskDTO": {
        "required": [
          "description",
          "dueDate"
        ],
        "type": "object",
        "properties": {
          "description": {
            "maxLength": 500,
            "minLength": 0,
            "type": "string"
          },
          "dueDate": {
            "type": "string",
            "format": "date-time"
          },
          "priority": {
            "maximum": 3,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "reminder": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "isCompleted": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "LoginDTO": {
        "required": [
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "maxLength": 100,
            "minLength": 6,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "NoteResponseDTO": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "description": "Note ID",
            "format": "int32"
          },
          "title": {
            "type": "string",
            "description": "Note title",
            "nullable": true
          },
          "content": {
            "type": "string",
            "description": "Note content",
            "nullable": true
          },
          "tags": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Tags associated with the note",
            "nullable": true
          },
          "color": {
            "type": "string",
            "description": "Color of the note",
            "nullable": true
          },
          "isPinned": {
            "type": "boolean",
            "description": "Whether the note is pinned"
          },
          "userId": {
            "type": "integer",
            "description": "ID of the user who created the note",
            "format": "int32"
          },
          "createdAt": {
            "type": "string",
            "description": "Creation date",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "description": "Last update date",
            "format": "date-time",
            "nullable": true
          },
          "sharedWith": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/NoteShareResponseDTO"
            },
            "description": "List of users who have shared the note",
            "nullable": true
          }
        },
        "additionalProperties": false,
        "description": "Data Transfer Object for note response data"
      },
      "NoteShareDTO": {
        "required": [
          "sharedWithUserId"
        ],
        "type": "object",
        "properties": {
          "sharedWithUserId": {
            "type": "integer",
            "description": "ID of the user to share the note with",
            "format": "int32"
          },
          "canEdit": {
            "type": "boolean",
            "description": "Whether the shared user can edit the note"
          }
        },
        "additionalProperties": false,
        "description": "Data Transfer Object for sharing a note"
      },
      "NoteShareResponseDTO": {
        "type": "object",
        "properties": {
